<p style="color: green"><%= notice %></p>

<h1>Tracks</h1>

<%= link_to "<<", admin_index_path %> |
<%= link_to "New track", new_admin_track_path %>

<div id="tracks">
  <% @tracks.each do |track| %>
    <%= render track %>
    <p>
      <%= link_to "Show this track", admin_track_path(track) %> |
      <%= link_to "Edit this track", edit_admin_track_path(track) %>
    </p>
  <% end %>
</div>
