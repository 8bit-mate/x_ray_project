<div id="<%= dom_id track %>">
  <p>
    <strong>Number:</strong>
    <%= track.number %>
  </p>

  <p>
    <strong>Song:</strong>
    <%= track.song.human_full_title %>
  </p>

  <%= render "audio", track: track %>

  <p>
    <strong>Record assigned:</strong>
    <%= render "associated_record", record: track.record %>
  </p>
</div>
