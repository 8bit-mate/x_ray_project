<h1>Editing label</h1>

<%= render "form", label: @label %>

<br>

<div>
  <%= link_to "Show this label", admin_label_path(@label) %> |
  <%= link_to "Back to labels", admin_labels_path %>

  <%= button_to "Destroy this label", [:admin, @label], method: :delete, class: "btn btn-danger btn-sm" %>
</div>
