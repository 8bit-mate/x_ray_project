<div id="<%= dom_id post %>" class="post">
  <div class="card">
    <div class="card-header">
      <h3 class="card-title">
        <%= post_title %>
      </h3>
    </div>
    <div class="card-body">
      <%= post.body %>
    </div>

    <ul class="list-group list-group-flush">
      <li class="list-group-item">
        Tags:
      </li>
    </ul>
  </div>
</div>